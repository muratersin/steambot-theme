{
	"name": "Steambot",
	"type": "dark",
	"colors": {
		// Base
		"focusBorder": "#4d4d4d",
		"foreground": "#f7f7f7",

		// Text Colors
		"textLink.activeForeground": "#963142",
		"textLink.foreground": "#afd661",

		// Editor		
		"editor.background": "#202020",
		"editor.foreground": "#d4d4d4",
		"editorLineNumber.activeForeground": "#963142",
		"editorLineNumber.foreground": "#4d4d4d",
		"editorCursor.background": "#afd661",
		"editorCursor.foreground": "#ff4040",

		// Button
		"button.background": "#afd661",
		"button.foreground": "#202020",
		"button.hoverBackground": "#4d4d4d",

		// Panel
		"panel.dropBackground": "#afd661",
		"panelTitle.activeForeground": "#963142",
		"panelTitle.inactiveForeground": "#d4d4d4",

		// List And Trees
		"list.activeSelectionBackground": "#4d4d4d",

		// Editor Group And Tabs
		"tab.activeForeground": "#afd661",
		"tab.inactiveForeground": "#d4d4d4",

		// Input
		"inputOption.activeBorder": "#afd661",
		"input.foreground": "#963142",


		// Activity Bar
		"activityBar.background": "#202020",
		"activityBar.foreground": "#afd661",
		"activityBarBadge.foreground": "#202020",
		"activityBarBadge.background": "#963142",

		// Side Bar
		"sideBar.background": "#202020",
		"sideBar.dropBackground": "#202020",

		// Notification
		"notifications.background": "#963142",
		"notifications.foreground": "#202020",

		// Badge
		"badge.background": "#afd661",
		"badge.foreground": "#000000",

		// Status Bar
		"statusBar.background": "#202020",
		"statusBar.foreground": "#afd661",
		"statusBar.debuggingBackground": "#202020",
		"statusBar.debuggingForeground": "#963142",
		"statusBar.noFolderBackground": "#202020",
		"statusBar.noFolderForeground": "#afd661",

		// Side Bar
		"sideBarSectionHeader.background": "#202020",
		"sideBarSectionHeader.foreground": "#963142"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#346d23"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"storage",
				"keyword"
			],
			"settings": {
				"foreground": "#afd661"
			}
		},
		{
			"name": "Text",
			"scope": [
				"variable",
				"meta",
				"support",
				"variable.parameter.handlebars"
			],
			"settings": {
				"foreground": "#f5f5f5"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"entity.other"
				// "punctuation"
			],
			"settings": {
				"foreground": "#afd661"
			}
		},
		{
			"name": "Entities",
			"scope": [
				"entity",
				"meta.function",
				"support.function"
			],
			"settings": {
				"foreground": "#f06a9b"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant"
			],
			"settings": {
				"foreground": "#b58efd"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#a8a8a8"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable.parameter",
				"support.constant.handlebars",
				"meta.template"
			],
			"settings": {
				"foreground": "#dd841e"
			}
		}
	]
}
{
  "name": "Steambot Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#1E2A24",
    "editor.foreground": "#E8E3E3",
    "editor.selectionBackground": "#6A8F74",
    "editor.lineHighlightBackground": "#243229",
    "editorCursor.foreground": "#FF8C42",
    "editorWhitespace.foreground": "#3A4F41",
    "editorIndentGuide.background1": "#3A4F41",
    "editorIndentGuide.activeBackground1": "#6A8F74",

    "sideBar.background": "#243229",
    "sideBar.foreground": "#B9C2B9",
    "sideBarSectionHeader.background": "#2E3E35",

    "activityBar.background": "#2E3E35",
    "activityBar.foreground": "#FF8C42",
    "activityBarBadge.background": "#FF8C42",
    "activityBarBadge.foreground": "#1E2A24",

    "titleBar.activeBackground": "#2E3E35",
    "titleBar.activeForeground": "#E8E3E3",

    "statusBar.background": "#243229",
    "statusBar.foreground": "#B9C2B9",
    "statusBarItem.hoverBackground": "#6A8F74"
  },
  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#808080",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword"],
      "settings": {
        "foreground": "#FF8C42",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#FF8C42",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#8FA88F"
      }
    },
    {
      "scope": ["variable", "variable.language"],
      "settings": {
        "foreground": "#e4e3e8"
      }
    },
    {
      "scope": ["function", "entity.name.function"],
      "settings": {
        "foreground": "#FFD67E"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#B9C2B9"
      }
    },
    {
      "scope": ["entity.name.tag", "support.class.component.html"],
      "settings": {
        "foreground": "#FFAA55",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#A0C3C3",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["meta.tag"],
      "settings": {
        "foreground": "#FF8C42"
      }
    },
    {
      "scope": ["text.html.basic"],
      "settings": {
        "foreground": "#E8E3E3"
      }
    },
    {
      "scope": [
        "meta.tag.inline.any",
        "meta.tag.inline.any entity.other.attribute-name",
        "meta.tag.inline.any string.quoted.double.html"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": ["entity.other.inline"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    }
  ]
}
